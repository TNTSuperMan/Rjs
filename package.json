{
  "name": "@tntsuperman/rjs",
  "version": "0.0.4",
  "description": "simple, react, fast JavaScript framework",
  "type": "module",
  "main": "dist/module.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "unpkg":    "dist/browser.js",
  "jsdelivr": "dist/browser.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:types",
    "build:lib": "npx rollup -c",
    "build:types": "npx tsc",
    "build:homepage": "cd homepage && npm install && npm run build",
    "prepare": "npm run build",
    "test": "vitest run"
  },
  "author": "TNTSuperMan",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jsdom": "^21.1.7",
    "jsdom": "^25.0.1",
    "rollup": "^4.24.0",
    "vitest": "^2.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TNTSuperMan/Rjs"
  },
  "homepage": "https://tntsuperman.github.io/Rjs"
}
